							<div class="form-group">
							    <input type="text"  class="form-control" placeholder="Enter Product Name" class="form-control" name="productname" required>
							</div>
							<div class="form-group">
								<input type="number"  placeholder="Enter Product Price" class="form-control"  name="productprice" required>     
							</div>
                            <div class="form-group">
								<input type="text"  placeholder="Enter Product Details" class="form-control"  name="productdetails" required>     
							</div>
                            <div class="form-group">
                                <input type="hidden" class="form-control"  name="shopid" value="<%=records%>">           
                           </div>
						   <div class="form-group">
							<input type="number"  placeholder="Quantity" class="form-control"  name="productquantity" required>     
							</div>
							<div class="form-group">
								<input type="file" value="Upload Product Image" class="form-control" name="file" required>
							</div>
							<!-- /row -->
							<hr>
							<div class="form-group">
								<input type="submit" name="submit" value="Add New Product" class="btn_1 full-width">
							</div>
							
