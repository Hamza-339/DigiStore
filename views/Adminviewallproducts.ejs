<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="Ansonika">
    <title>DIGI-STORE</title>

    <!-- Favicons-->
    <link rel="shortcut icon" href="/img/dlogo.png" type="image/x-icon">
    <link rel="apple-touch-icon" type="image/x-icon" href="/img/apple-touch-icon-57x57-precomposed.png">
    <link rel="apple-touch-icon" type="image/x-icon" sizes="72x72" href="/img/apple-touch-icon-72x72-precomposed.png">
    <link rel="apple-touch-icon" type="image/x-icon" sizes="114x114" href="/img/apple-touch-icon-114x114-precomposed.png">
    <link rel="apple-touch-icon" type="image/x-icon" sizes="144x144" href="/img/apple-touch-icon-144x144-precomposed.png">
	
    <!-- GOOGLE WEB FONT -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700,900&display=swap" rel="stylesheet">

    <!-- BASE CSS -->
    <link href="/css/bootstrap.custom.min.css" rel="stylesheet">
    <link href="/css/style.css" rel="stylesheet">

	<!-- SPECIFIC CSS -->
    <link href="/css/listing.css" rel="stylesheet">

    <!-- YOUR CUSTOM CSS -->
    <link href="/css/custom.css" rel="stylesheet">

	<link src="http://maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet">
	<link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet">
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">

	<!--NEW TABLE HEAD-->

	<link href='/https://fonts.googleapis.com/css?family=Roboto:400,100,300,700' rel='stylesheet' type='text/css'>

	<link rel="stylesheet" href="/https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">
	
	<link rel="stylesheet" href="/css/styles.css">

</head>

<body>
	
	<div id="page">
		
	<header class="version_1">
		<div class="layer"></div><!-- Mobile menu overlay mask -->
		<div class="main_header">
			<div class="container">
				<div class="row small-gutters">
					<div class="col-xl-3 col-lg-3 d-lg-flex align-items-center">
						<div id="logo">
							<a href="#"><img src="/img/dlogo.png" alt="" width="20%" height="20%"></a>
						</div>
					</div>
					<nav class="col-xl-6 col-lg-7">
						<a class="open_close" href="javascript:void(0);">
							<div class="hamburger hamburger--spin">
								<div class="hamburger-box">
									<div class="hamburger-inner"></div>
								</div>
							</div>
						</a>
						<!-- Mobile menu button -->
						<div class="main-menu">
							<div id="header_menu">
								<a href="#"><img src="/img/dlogo.png" alt="" width="100" height="35"></a>
								<a href="#" class="open_close" id="close_in"><i class="ti-close"></i></a>
							</div>
							<ul style="width: 700px;">
								<li>
									<a href="/Admindashboard?id=<%=admin_id%>">Dashboard</a>
								</li>
								<li>
									<a href="/Adminsellerstores/<%=admin_id%>">Store Management</a>
								</li>
								<li>
									<a href="/Viewregisteredusers/<%=admin_id%>">User Management</a>
								</li>
								<li>
									<a href="/Adminviewfeedback/<%=admin_id%>">Feedback</a>
								</li>
								<li>
									<a href="/logout">LOG OUT</a>
								</li>
							</ul>
						</div>
						<!--/main-menu -->
					</nav>
					<div class="col-xl-3 col-lg-2 d-lg-flex align-items-center justify-content-end text-right">
						<%if(loginUser!=" "){%>
							<a class="phone_top" href="#">Logged in as:  <%= loginUser %></a>
						<%}%>
					</div>
				</div>
				<!-- /row -->
			</div>
		</div>
		<!-- /main_header -->

	</header>
	<!-- /header -->
		
	<main>
		<div class="top_banner version_2">
			<div class="opacity-mask d-flex align-items-center" data-opacity-mask="rgba(0, 0, 0, 0)">
				<div class="container">
					<div class="d-flex justify-content-center"><h1 style="background: none;"><%=record.shopname%></h1></div>
				</div>
			</div>
			<img src="/img/Banner22.jpg"  data-src="/img/blackbackground.png" class="img-fluid" alt="">
		</div>
		<!-- /top_banner -->

			<!-- /toolbox -->
			<section class="ftco-section">
				<div class="container">
		
					<div class="row">
						<div class="col-md-12">
							<h3 class="h5 mb-4 text-center">Orders</h3>
							<div class="table-wrap">
								<table class="table">
								  <thead class="thead-primary">
									<tr>
									  <th>&nbsp;</th>
									  <th>Product</th>
									  <th>Price</th>
									  <th>Quantity</th>
									  <th>User</th>
									  <th>Delivery Address</th>
									</tr>
								  </thead>
								  <tbody>
									  <%if(orders.length>0){
										orders.forEach(function(row){%>
									<tr class="alert" role="alert">
									  <td>
											<div class="img" style = "background-image: url(/uploads/<%=row.product_id.productimage%>);"></div>
									  </td>
									  <td>
										  <div class="email">
											  <span style="color: black;"><%=row.product_id.productname%></span>
										  </div>
									  </td>
									  <td><%=row.product_id.productprice%> Rs</td>
									  <td><%=row.quantity%></td>
									  
									  <td><%=row.user_id.username%></td>
									  
									  <td><%=row.user_id.address%></td>

		
									</tr>
		
										<% })} %>
								  </tbody>
								</table>
							</div>
						</div>
					</div>
				</div>
			</section>
		
		  <script src="/js/jquery.min.js"></script>
		  <script src="/js/popper.js"></script>
		  <script src="/js/bootstrap.min.js"></script>
		  <script src="/js/main.js"></script>

			<div class="container margin_30">
				<div class="row small-gutters">
				<%if(records.length>0){
					records.forEach(function(row){%>
				
				<div class="col-6 col-md-4 col-xl-3">
					<div class="grid_item">

						<figure>
							<a href="#">
								<img class="img-fluid lazy" src="/uploads/<%=row.productimage%>" data-src="/uploads/<%=row.productimage%>" alt="">
							</a>
						</figure>
						<a href="#">
							<h3><%=row.productname%></h3>
						</a>
						<div class="price_box">
							<span class="new_price"><%=row.productprice%> Rs</span>
						</div>
						<ul>
							<li><a href="/Admindeleteproduct/delete/<%=row._id%>/<%=row.shop_id%>/<%=user_id%>/<%=admin_id%>" class="tooltip-1" data-toggle="tooltip" data-placement="left" title="Delete Product"><i class="fa fa-trash-o"></i><span>Delete Product</span></a></li>
						</ul>
					</div>
					<!-- /grid_item -->
				</div>
				<!-- /col -->
				
				<!-- /col -->				
			<% })} %>
			<!-- /row -->
				</div>
		</div>
		<!-- /container -->
	</main>
	<!-- /main -->
	
	<footer class="revealed">
		<div class="container">
			<div class="row">
				<div class="col-lg-3 col-md-6">
					<h3 data-target="#collapse_1">Quick Links</h3>
					<div class="collapse dont-collapse-sm links" id="collapse_1">
						<ul>
							<li><a href="/Admindashboard?id=<%=admin_id%>">Dashboard</a></li>
							<li><a href="/Adminsellerstores/<%=admin_id%>">Stores</a></li>
							<li><a href="/Viewregisteredusers/<%=admin_id%>">Users</a></li>
							<li><a href="/Adminviewfeedback/<%=admin_id%>">Feedback</a></li>
						</ul>
					</div>
				</div>
				<div class="col-lg-3 col-md-6">
					<h3 data-target="#collapse_2">Users</h3>
					<div class="collapse dont-collapse-sm links" id="collapse_2">
						<ul>
							<li><a href="/logout">LOG OUT</a></li>
						</ul>
					</div>
				</div>
				<div class="col-lg-3 col-md-6">
						<h3 data-target="#collapse_3">Contacts</h3>
					<div class="collapse dont-collapse-sm contacts" id="collapse_3">
						<ul>
							<li><i class="ti-headphone-alt"></i>+92 301-2345678</li>
							<li><i class="ti-email"></i><a href="#0">info@digistore.com</a></li>
						</ul>
					</div>
				</div>
			</div>
			<!-- /row-->
			<hr>
			<div class="row add_bottom_25" style="width: 800px; margin-left: 350px;">
				<div class="col-lg-6">
					<ul class="additional_links">
						<li><a href="#0">Terms and conditions</a></li>
						<li><a href="#0">Privacy</a></li>
						<li><span>Â© 2021 DIGI-STORE</span></li>
					</ul>
				</div>
			</div>
		</div>
	</footer>
	<!--/footer-->
	</div>
	<!-- page -->
	
	<div id="toTop"></div><!-- Back to top button -->
	
	<!-- COMMON SCRIPTS -->
    <script src="/js/common_scripts.min.js"></script>
    <script src="/js/main.js"></script>
	
	<!-- SPECIFIC SCRIPTS -->
	<script src="/js/sticky_sidebar.min.js"></script>
	<script src="/js/specific_listing.js"></script>
		
</body>
</html>