<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="Ansonika">
    <title>DIGI-STORE</title>

    <!-- Favicons-->
    <link rel="shortcut icon" href="/img/dlogo.png" type="image/x-icon">
    <link rel="apple-touch-icon" type="image/x-icon" href="img/apple-touch-icon-57x57-precomposed.png">
    <link rel="apple-touch-icon" type="image/x-icon" sizes="72x72" href="img/apple-touch-icon-72x72-precomposed.png">
    <link rel="apple-touch-icon" type="image/x-icon" sizes="114x114" href="img/apple-touch-icon-114x114-precomposed.png">
    <link rel="apple-touch-icon" type="image/x-icon" sizes="144x144" href="img/apple-touch-icon-144x144-precomposed.png">
	
    <!-- GOOGLE WEB FONT -->
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500,700,900&display=swap" rel="stylesheet">

    <!-- BASE CSS -->
    <link href="css/bootstrap.custom.min.css" rel="stylesheet">
    <link href="css/style.css" rel="stylesheet">

	<!-- SPECIFIC CSS -->
    <link href="css/listing.css" rel="stylesheet">

    <!-- YOUR CUSTOM CSS -->
    <link href="css/custom.css" rel="stylesheet">

	<link src="http://maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet">
	<link href="http://maxcdn.bootstrapcdn.com/font-awesome/4.1.0/css/font-awesome.min.css" rel="stylesheet">
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">


</head>

<body>
	
	<div id="page" class="theia-exception">
		
	<header class="version_1">
		<div class="layer"></div><!-- Mobile menu overlay mask -->
		<div class="main_header">
			<div class="container">
				<div class="row small-gutters">
					<div class="col-xl-3 col-lg-3 d-lg-flex align-items-center">
						<div id="logo">
							<a href="#"><img src="/img/dlogo.png" alt="" width="20%" height="20%"></a>
						</div>
					</div>
					<nav class="col-xl-6 col-lg-7">
						<a class="open_close" href="javascript:void(0);">
							<div class="hamburger hamburger--spin">
								<div class="hamburger-box">
									<div class="hamburger-inner"></div>
								</div>
							</div>
						</a>
						<!-- Mobile menu button -->
						<div class="main-menu">
							<div id="header_menu">
								<a href="#"><img src="dlogo.png" alt="" width="100" height="35"></a>
								<a href="#" class="open_close" id="close_in"><i class="ti-close"></i></a>
							</div>
							<ul>
								<li>
									<a href="#">Dashboard</a>
								</li>
								<li>
									<a href="/Sellerstores/<%=user_id%>">Stores</a>
								</li>
								<li>
									<a href="/Sellerfeedback/<%=user_id%>">Feedback</a>
								</li>
								<li>
									<a href="/logout">LOG OUT</a>
								</li>
							<!--	<li>
									<a href="/logout">Log out</a>
								</li>-->
							</ul>
						</div>
						<!--/main-menu -->
					</nav>
					<div class="col-xl-3 col-lg-2 d-lg-flex align-items-center justify-content-end text-right">
						<%if(loginUser!=" "){%>
							<a class="phone_top" href="#">Logged in as:  <%= loginUser %> (Seller)</a>
						<%}%>
					</div>
				</div>
				<!-- /row -->
			</div>
		</div>
		<!-- /main_header -->


		<!-- /main_nav -->
	</header>
	<!-- /header -->
		
	<main>

	    <!-- /top_banner -->
	    <div id="stick_here"></div>
		<script type="text/javascript">
			var redirectionUrl='';
			var urlParams = new URLSearchParams(window.location.search);
	
			if (urlParams.has('id')) {//check krega k id k name se koi parameter ha
				redirectionUrl=urlParams.get('id')//agr ha to get kar k redirectionurl me store kr de
			}
		</script>

	    <div class="toolbox elemento_stick">
	        <div class="container">		
				<button class="btn_1 full-width" id="createStore" onclick="window.open('/Createstore?id='+redirectionUrl,'_self');">Create New Store </button>
			<!--
					<button class="btn_1" ><a id="mylink1" href="#" onclick="return changeLink1();" style="color: white;">View All Stores</a></button>
			-->
	            <ul class="clearfix">
	                <li>
	                    <div class="sort_select">
	                            <p> Tap to add new store </p>
	                    </div>
	                </li>
	                <li>
	                    <a href="#"><i class="ti-view-grid"></i></a>
	                    <a href="#"><i class="ti-view-list"></i></a>
	                </li>
	                <li>
	                    <a href="#" class="open_filters">
	                        <i class="ti-filter"></i><span>Filters</span>
	                    </a>
	                </li>
	            </ul>
	        </div>
	    </div>
	    <!-- /toolbox -->
	    <div class="container margin_30">
	        <div class="row">
				
	            <div class="col-lg-12">
							
		
<%if(records.length>0){
	records.forEach(function(row){%>
				<div class="row row_item">
					<div class="col-sm-4">
						<figure>
							<a href="#">
								<img class="img-fluid lazy loaded" src="uploads/<%=row.imagename%>"  alt="No Image Found">
							</a>
						</figure>
					</div>
					<div class="col-sm-8">
						 <a href="#">
							<h3><%=row.shopname%></h3>
						</a>
						<p><%=row.shopaddress%></p>
						<ul>
							<li><a href="/Viewallproducts/<%=row._id%>/<%=row.user_id%>" class="btn_1">View Shop</a></li>
							<li><a href="/Shopdetail/delete/<%=row._id%>/<%=row.user_id%>" class="btn_1 gray tooltip-1" data-toggle="tooltip" data-placement="top" title="" data-original-title="Delete Shop"><i class="fa fa-trash-o" ></i><span>Add to favorites</span></a></li>
							<li><a href="/Shopdetail/UpdateStore/<%=row._id%>/<%=row.user_id%>" class="btn_1 gray tooltip-1" data-toggle="tooltip" data-placement="top" title="" data-original-title="Update Shop Details"><i class="fa fa-edit"></i><span>Add to compare</span></a></li>
		
						</ul>
					</div>
				</div>
<%    
	})} %>
				</div>
	            <!-- /col -->

	            <!-- /col -->
	        </div>
	        <!-- /row -->
	    </div>
	    <!-- /container -->
	</main>
	<!-- /main -->
	
	<footer class="revealed">
		<div class="container">
			<div class="row">
				<div class="col-lg-3 col-md-6">
					<h3 data-target="#collapse_1">Quick Links</h3>
					<div class="collapse dont-collapse-sm links" id="collapse_1">
						<ul>
							<li><a href="/#">Dashboard</a></li>
							<li><a href="/Sellerstores/60d2e95fb0ba5d2a78f56df0">Stores</a></li>
						</ul>
					</div>
				</div>
				<div class="col-lg-3 col-md-6">
					<h3 data-target="#collapse_2">Users</h3>
					<div class="collapse dont-collapse-sm links" id="collapse_2">
						<ul>
							<li><a href="/logout">LOG OUT</a></li>
						</ul>
					</div>
				</div>
				<div class="col-lg-3 col-md-6">
						<h3 data-target="#collapse_3">Contacts</h3>
					<div class="collapse dont-collapse-sm contacts" id="collapse_3">
						<ul>
							<li><i class="ti-headphone-alt"></i>+92 301-2345678</li>
							<li><i class="ti-email"></i><a href="#0">info@digistore.com</a></li>
						</ul>
					</div>
				</div>
			</div>
			<!-- /row-->
			<hr>
			<div class="row add_bottom_25" style="width: 800px; margin-left: 350px;">
				<div class="col-lg-6">
					<ul class="additional_links">
						<li><a href="#0">Terms and conditions</a></li>
						<li><a href="#0">Privacy</a></li>
						<li><span>Â© 2021 DIGI-STORE</span></li>
					</ul>
				</div>
			</div>
		</div>
	</footer>
	<!--/footer-->
	</div>
	<!-- page -->
	
	<div id="toTop"></div><!-- Back to top button -->
	
	<!-- COMMON SCRIPTS -->
    <script src="js/common_scripts.min.js"></script>
    <script src="js/main.js"></script>
	
	<!-- SPECIFIC SCRIPTS -->
	<script src="js/sticky_sidebar.min.js"></script>
	<script src="js/specific_listing.js"></script>
		
</body>
</html>