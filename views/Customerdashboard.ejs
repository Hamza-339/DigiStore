            </div>


			<div class="container margin_30">
				<div class="row small-gutters">
				<%if(shopinfo.length>0){
					shopinfo.forEach(function(row){%>
				
				<div class="col-6 col-md-4 col-xl-3">
					<div class="grid_item">

						<figure>
							<a href="#">
								<img class="img-fluid lazy" src="/uploads/<%=row.imagename%>" data-src="/uploads/<%=row.imagename%>" alt="">
							</a>
						</figure>
						<a href="#">
							<h3><%=row.shopname%></h3>
						</a>
						<div class="price_box">
							<span class="new_price">Address: <%=row.shopaddress%> </span>
						</div>
						<%if(user_id){%>
							<ul>
								<li><a href="/Customerviewallproducts/<%=row._id%>/<%=user_id%>" class="tooltip-1" data-toggle="tooltip" data-placement="left" title="View Shop Products"><i class="fa fa-eye"></i><span>View Shop Products</span></a></li>
							</ul>
						<% } %>	
					</div>
					<!-- /grid_item -->
				</div>
				<!-- /col -->
				
				<!-- /col -->				
			<% })} %>
			<!-- /row -->
				</div>	
			
				
			</div>
			<!-- /Different shop products -->
				<img src="/img/qqq.png"  data-src="/img/qqq.png" height="450px" width="100%"  alt="">

			<div class="page_header">
					<h1 style="text-align: center;color: #084cdc;font-family: fantasy;font-size: 50px;margin-top: 4%;">Best Quality Products</h1>
			</div>
			<div class="container margin_30">
				<div class="row small-gutters">
				<%if(records.length>0){
					records.forEach(function(row1){%>
				
				<div class="col-6 col-md-4 col-xl-3">
					<div class="grid_item">

						<figure>
							<a href="#">
								<img class="img-fluid lazy" src="/uploads/<%=row1.productimage%>" data-src="/uploads/<%=row1.productimage%>" alt="">
							</a>
						</figure>
						<a href="#">
							<h3><%=row1.productname%></h3>
						</a>
						<div class="price_box">
							<span class="new_price"><%=row1.productprice%> PKR</span>
						</div>
						<%if(shoprecord.length>0){
							shoprecord.forEach(function(shop){%>
								 
							<%
								 var a= row1.shop_id; 
								 var b= shop._id
								 var result=a.toString().localeCompare(b); 
								 var compareResult=false;
								 if(!result) compareResult=true;
							%>
							
							<%if(compareResult){%>
								<%if(user_id){%>
									<div class="price_box">
										<span class="new_price"><u><a href="/Customerviewallproducts/<%=shop._id%>/<%=user_id%>"><%=shop.shopname%></a></u></span>
									</div>
								<% } %>	
							<% } %>
						<% })} %>		

						<%if(user_id){%>
								<ul>
									<li><a href="/Customerviewproductdetails/<%=row1._id%>/<%=user_id%>" class="tooltip-1" data-toggle="tooltip" data-placement="left" title="View Product Details"><i class="fa fa-info"></i><span>View Product Details</span></a></li>
								</ul>
						<% } %>	

					</div>
					<!-- /grid_item -->
				</div>
				<!-- /col -->
				
				<!-- /col -->				
			<% })} %>

