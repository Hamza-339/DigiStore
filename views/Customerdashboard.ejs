	<script>
		$(function(){
			$('#Searchproducts').autocomplete({
				source: function(req,res){
					$.ajax({
						url:"/Autocomplete/",
						dataType:"jsonp",
						type:"GET",
						data:req,
						success:function(data){
							res(data);
						}
					});
				},
				minLength:1,
				select:function(event,ui){
					if(ui.item){
						$('#Searchproducts').text(ui.item.label);
					}
				}
			});
		});
	</script>
		
