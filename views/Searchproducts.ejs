					<div class="col-xl-6 col-lg-7 col-md-6 d-none d-md-block">
						<div class="custom-search-input">
                     <!--       <form action="Searchproducts/<%=records._id%>"  method="post"  role="form" enctype="multipart/form-data">
                         -->    
                            <form action="/Searchproducts/<%=user_id%>"  method="post"  role="form" enctype="multipart/form-data">
                                
                                
								<div class="custom-search-input">
									<input type="text" id="Searchproducts" placeholder="Search over 10.000 products" name="productname" >
                              
									<button type="submit" ><i class="header-icon_search_custom"></i></button>
								</div>
								
                            </form>
						</div>
					</div>
					<div class="col-xl-3 col-lg-2 col-md-3">
						<ul class="top_tools">
						</ul>
					</div>
				</div>
				<!-- /row -->
			</div>
			<div class="search_mob_wp">
				<input type="text" class="form-control" placeholder="Search over 10.000 products">
				<input type="submit" class="btn_1 full-width" value="Search">
			</div>
			<!-- /search_mobile -->
		<!-- /main_nav -->
		</div>
	</header>
	<!-- /header -->
		
	<main>
	<!--	<img src="/img/coverr.png"  data-src="/img/coverr.png" class="img-fluid" alt="" style="margin-top: 27px;">-->

		<!-- /top_banner -->
		
			<div id="stick_here"></div>		
            <div class="p-3 mb-2 bg-dark text-white">Shop From Your Favourite Store</div>

			<!-- /toolbox -->


			<div class="container margin_30">
				<div class="row small-gutters">
				<%if(records.length>0){
					records.forEach(function(row){%>
				
				<div class="col-6 col-md-4 col-xl-3">
					<div class="grid_item">

						<figure>
							<a href="#">
								<img class="img-fluid lazy" src="/uploads/<%=row.productimage%>" data-src="/uploads/<%=row.productimage%>" alt="">
							</a>
						</figure>
						<a href="#">
							<h3><%=row.productname%></h3>
						</a>
						<div class="price_box">
							<span class="new_price"><%=row.productprice%> PKR</span>
						</div>
						<%if(shoprecord.length>0){
							shoprecord.forEach(function(shop){%>
								 
							<%
								 var a= row.shop_id; 
								 var b= shop._id
								 var result=a.toString().localeCompare(b); 
								 var compareResult=false;
								 if(!result) compareResult=true;
							%>
							
							<%if(compareResult){%>
									<div class="price_box">
										<span class="new_price"><u><a href="/Customerviewallproducts/<%=shop._id%>/<%=user_id%>"><%=shop.shopname%></a></u></span>
									</div>
								<% } %>
						<% })} %>		


						<ul>
							<li><a href="/Customerviewproductdetails/<%=row._id%>/<%=user_id%>" class="tooltip-1" data-toggle="tooltip" data-placement="left" title="View Product Details"><i class="fa fa-info"></i><span>View Product Details</span></a></li>
						</ul>
					</div>
					<!-- /grid_item -->
				</div>
				<!-- /col -->
				
				<!-- /col -->				
			<% })} %>

			<!-- /row -->
				</div>	
			<div class="pagination__wrapper">
				<ul class="pagination">
					<li><a href="#0" class="prev" title="previous page">&#10094;</a></li>
					<li>
						<a href="#0" class="active">1</a>
					</li>
					<li>
						<a href="#0">2</a>
					</li>
					<li>
						<a href="#0">3</a>
					</li>
					<li>
						<a href="#0">4</a>
					</li>
					<li><a href="#0" class="next" title="next page">&#10095;</a></li>
				</ul>
			</div>
				
		</div>
		<!-- /container -->
	</main>
	<!-- /main -->
	
